<h1>Sign Up</h1>
<%= form_with model: @admin, url: registrations_new_path do |f| %>
  <p>
  <%= f.label 'user name' %><br>
  <%= f.text_field :name %>
  </p>
  <p>
  <%= f.label 'password' %><br>
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.label 'password_confirmation' %><br>
  <%= f.password_field :password_confirmation %>
  </p>
  <p>
  <%= f.submit 'Sign Up' %>
  </p>
<% end %>